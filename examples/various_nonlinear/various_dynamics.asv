function [dydt] = various_dynamics(t,y)
 
    global CONST
    
    y = reshape(y, [], 1);
    
    switch CONST.choose
        case 'one'
            dydt    = y.^2;
        case 'two'
            dydt    = t.*sin(y) + y.*cos(t);
        case 'three'    % 
            dydt    = [y(2);...
                        (1-y(1)^2)*y(2) - y(1)];
        otherwise
            disp(['No equation found for ' CONST.choose ])
    end
return